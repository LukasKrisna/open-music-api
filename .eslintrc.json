{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "space-before-blocks": "off",
        "require-jsdoc": "off",
        "padded-blocks": "off",
        "indent": "off",
        "max-len": "off",
        "no-trailing-spaces": "off",
        "import/no-unresolved": "off",
    }
}